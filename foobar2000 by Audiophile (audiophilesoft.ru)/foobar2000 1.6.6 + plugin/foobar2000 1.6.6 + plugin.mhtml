:root { --palette-vk-font:-apple-system, BlinkMacSystemFont, Roboto, Open Sans, Helvetica Neue, "Noto Sans Armenian", "Noto Sans Bengali", "Noto Sans Cherokee", "Noto Sans Devanagari", "Noto Sans Ethiopic", "Noto Sans Georgian", "Noto Sans Hebrew", "Noto Sans Kannada", "Noto Sans Khmer", "Noto Sans Lao", "Noto Sans Osmanya", "Noto Sans Tamil", "Noto Sans Telugu", "Noto Sans Thai", sans-serif; }

.ActivityIndicator { display: inline-flex; -webkit-box-align: center; align-items: center; height: 1em; overflow: hidden; }

.ActivityIndicator::after { text-align: left; content: ".\a ..\a ..."; display: inline-block; white-space: pre-wrap; animation: ActivityIndicatorAnimation 1s steps(4, end) 0s infinite normal none running; transform: translateY(33.33%); }

[dir="rtl"] .ActivityIndicator::after { text-align: right; }

@supports ((-webkit-clip-path:inset(0 0)) or (clip-path:inset(0 0))) {
  .ActivityIndicator { overflow: visible; clip-path: inset(-20% 0px); }
}

@-webkit-keyframes ActivityIndicatorAnimation { 
  0% { transform: translateY(33.33%); }
  24.99% { transform: translateY(33.33%); }
  25% { transform: translateY(0px); }
  49.99% { transform: translateY(0px); }
  50% { transform: translateY(-33.33%); }
  99.99% { transform: translateY(-33.33%); }
  100% { transform: translateY(33.33%); }
}

@keyframes ActivityIndicatorAnimation { 
  0% { transform: translateY(33.33%); }
  24.99% { transform: translateY(33.33%); }
  25% { transform: translateY(0px); }
  49.99% { transform: translateY(0px); }
  50% { transform: translateY(-33.33%); }
  99.99% { transform: translateY(-33.33%); }
  100% { transform: translateY(33.33%); }
}

.Alert { border-radius: 4px; padding: 10px 15px; display: flex; line-height: 18px; }

.Alert--done { background: var(--content_positive_background); }

.Alert--wrong { background: var(--content_negative_background); }

.Alert--warning { background: var(--content_warning_background); }

.Alert--info { background: var(--content_tint_background); }

.Alert__icon { padding: 5px 14px 0px 0px; }

[dir="rtl"] .Alert__icon { padding: 5px 0px 0px 14px; }

.Alert--done .Alert__icon { color: var(--green_400); }

.Alert--wrong .Alert__icon { color: var(--red); }

.Alert--warning .Alert__icon { color: var(--orange); }

.Avatar { display: block; width: 100%; position: relative; transform: translateZ(0px); }

.Avatar--online { border-color: inherit; }

.Avatar--online::after { content: ""; display: block; height: 8px; width: 8px; position: absolute; bottom: 0px; right: 0px; border-width: 2px; border-style: solid; border-radius: 50%; border-color: var(--avatar-online-pane, inherit); background-color: var(--green_350); }

