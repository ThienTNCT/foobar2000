.wall_module .wcommunity_post_wide .post_header_info .ui_actions_menu_icons { top: 5px; right: 0px; }

[dir="rtl"] .wall_module .wcommunity_post_wide .post_header_info .ui_actions_menu_icons { right: auto; left: 0px; }

.wall_module .wcommunity_post_wide .post_header_info .shown .ui_actions_menu { top: 26px; }

.wall_module .wcommunity_post_wide .post_header_info .ui_actions_menu { top: 36px; right: -6.5px; }

[dir="rtl"] .wall_module .wcommunity_post_wide .post_header_info .ui_actions_menu { right: auto; left: -6.5px; }

.wall_module .wcommunity_post_wide .post_header_info .ui_actions_menu::after, .wall_module .wcommunity_post_wide .post_header_info .ui_actions_menu::before { right: 16.5px; }

[dir="rtl"] .wall_module .wcommunity_post_wide .post_header_info .ui_actions_menu::after, [dir="rtl"] .wall_module .wcommunity_post_wide .post_header_info .ui_actions_menu::before { right: auto; left: 16.5px; }

.wall_module .wcommunity_post_wide:first-child { border-top: 0px; padding-top: 0px; }

.wall_module .wcommunity_post_wide .wall_text { padding-left: 0px; padding-right: 0px; padding-bottom: 10px; }

.wall_module .wcommunity_post_wide .post_full_like_wrap { margin-left: 0px; margin-right: 0px; padding-top: 0px; padding-bottom: 7px; border-top: 0px; }

.wall_module .wcommunity_post_wide .post_full_like_wrap .post_like:hover { background: 0px 0px; }

#page_wrap .wcommunity_like_tt::after, #page_wrap .wcommunity_like_tt::before { right: 23px; }

[dir="rtl"] #page_wrap .wcommunity_like_tt::after, [dir="rtl"] #page_wrap .wcommunity_like_tt::before { right: auto; left: 23px; }

#page_wrap .wcommunity_post_like_tt::after, #page_wrap .wcommunity_post_like_tt::before { left: 14px; }

[dir="rtl"] #page_wrap .wcommunity_post_like_tt::after, [dir="rtl"] #page_wrap .wcommunity_post_like_tt::before { left: auto; right: 14px; }

.wall_module .wcommunity_post_wide .wall_text .post_media_voting:last-child { margin: 15px 0px 7px; }

.wall_module .wcommunity_post_wide.post:hover .wall_post_text { cursor: auto; }

.widget_community_confirm_text { overflow-wrap: break-word; }

.widget_community_confirm_box { text-align: center; }

.widget_community_confirm_box img { vertical-align: top; width: 100px; height: 100px; border-radius: 50%; background-color: rgb(240, 242, 245); position: relative; margin-bottom: 20px; }

.widget_community_confirm_box img::before { content: " "; display: block; position: absolute; top: 0px; left: 0px; width: 100px; height: 100px; background-color: rgb(255, 255, 255); }

[dir="rtl"] .widget_community_confirm_box img::before { left: auto; right: 0px; }

.widget_community_confirm_box img::after { width: 100px; height: 100px; border-radius: 50%; content: " "; display: block; position: absolute; top: 0px; left: 0px; background-color: rgb(240, 242, 245); }

[dir="rtl"] .widget_community_confirm_box img::after { left: auto; right: 0px; }

.wcommunity_nano .eltt.post_author_data, .wcommunity_mini .eltt.post_author_data { max-width: 274px; }

.wcommunity_nano .eltt.post_author_data { min-width: 200px; left: -65px !important; }

[dir="rtl"] .wcommunity_nano .eltt.post_author_data { left: auto !important; right: -65px !important; }

.wcommunity_nano .eltt.post_author_data .eltt_arrow_back { left: 65px !important; }

[dir="rtl"] .wcommunity_nano .eltt.post_author_data .eltt_arrow_back { left: auto !important; right: 65px !important; }

.wcommunity_wall_posts .like_wrap { padding: 0px; }

.wcommunity_wall_posts .like_cont { padding: 8px 0px; box-shadow: none; }

.likes_tt_like::after { left: 20px !important; }

[dir="rtl"] .likes_tt_like::after { left: auto !important; right: 20px !important; }

.wcommunity_mini .media_voting_question { line-height: 32px; max-height: 96px; font-size: 24px; letter-spacing: var(--vk-sans-ls-2); font-family: var(--vk-sans-display); }

.wcommunity_nano .media_voting_question { line-height: 20px; max-height: 63px; font-size: 20px; letter-spacing: var(--vk-sans-ls-1); font-family: var(--vk-sans-display); }

.wcommunity_nano .media_voting_header { padding: 0px 20px; }
